#: appModules/radiosure/__init__.py:236 appModules/radiosure/__init__.py:509
#, python-format
msgid "%s percent"
msgstr "%s por ciento"

#: appModules/radiosure/__init__.py:260
msgid "Stations"
msgstr "Emisoras"

#: appModules/radiosure/__init__.py:261
msgid "Search"
msgstr "Búsqueda"

#: appModules/radiosure/__init__.py:308
msgid "Are you sure you want  to quit RadioSure?"
msgstr "¿Estás seguro de que quieres salir de RadioSure?\" "

#: appModules/radiosure/__init__.py:310
#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:107
#: shared/rs_addonConfigManager.py:185
msgid "Warning"
msgstr "Advertencia"

#: appModules/radiosure/__init__.py:330
msgid "Move focus to Exit button and click it to exit the application"
msgstr "Haga clic en el botón Exit para salir de la aplicación."

#: appModules/radiosure/__init__.py:340
msgid ""
"Without moving focus, click on  Play button to play or stop the current "
"station"
msgstr ""
"Sin mover el foco, haga clic en el botón Play para reproducir o detener la "
"emisora actual."

#: appModules/radiosure/__init__.py:352
msgid ""
"Without moving focus, click on Back button to play the prior played station"
msgstr ""
"Sin mover el foco, haga clic en el botón Back para reproducir la emisora "
"anterior en la lista de las emisoras ya jugadas."

#: appModules/radiosure/__init__.py:364
msgid ""
"Without moving focus, click on  Next button to play the next played station"
msgstr ""
"Sin mover el foco, haga clic en el botón Next para reproducir la emisora "
"siguiente en la lista de las emisoras ya jugadas."

#: appModules/radiosure/__init__.py:374
msgid "Without moving focus, click on  Rec button to start or stop record"
msgstr ""
"Sin mover el foco, haga clic en el botón Rec para iniciar o detener la "
"grabación de la emisora actual."

#: appModules/radiosure/__init__.py:392
msgid "Search edit box and Stations list are not more visible"
msgstr "El cuadro de búsqueda y la lista de emisoras ya no son visibles"

#: appModules/radiosure/__init__.py:401
msgid ""
"Without moving focus, click  on Expand button to  show or mask stations list"
msgstr ""
"Sin mover el foco, haga clic en el botón Expand para mostrar o ocultar la "
"lista de emisoras."

#: appModules/radiosure/__init__.py:413
msgid "Move focus to Options button and click it to open Options dialog"
msgstr ""
"Haga clic en el botón Options para mostrar el cuadro de diálogo de "
"configuración de opciones."

#: appModules/radiosure/__init__.py:427
msgid ""
"Move focus to Fav button and click it to show the list of favorites station"
msgstr "Haga clic en el botón Fav para abrir la lista de emisoras favoritas."

#: appModules/radiosure/__init__.py:436
msgid "Without moving focus, click on Top buttonto ???"
msgstr "Sin mover el foco, haga clic en el botón  Top para???"

#: appModules/radiosure/__init__.py:447
msgid ""
"Without moving focus, click on Mute butt button to Mute volume on or off"
msgstr ""
"Sin mover el foco, haga clic en el botón Mute para poner o silenciar el "
"sonido."

#: appModules/radiosure/__init__.py:454
msgid "Decrease slightly the volume(down 3%)"
msgstr "Disminuya ligeramente el volumen ( menos 3%)"

#: appModules/radiosure/__init__.py:461
msgid "Increase slightly the volume(up 3%)"
msgstr "Aumenta ligeramente el volumen (más 3%)"

#: appModules/radiosure/__init__.py:469
msgid "Decrease the volume(down 5%)"
msgstr "Disminuya medianamente el volumen ( menos 5%)"

#: appModules/radiosure/__init__.py:476
msgid "Increase the volume(up 5%)"
msgstr "Aumenta medianamente el volumen (más 5%)"

#: appModules/radiosure/__init__.py:483
msgid "Increase strongly  the volume(up 20%)"
msgstr "Aumenta fuertemente el volumen (más 20%)"

#: appModules/radiosure/__init__.py:490
msgid "Decrease strongly the volume(down -20%)"
msgstr "Disminuya fuertemente el volumen (menoss 20%)"

#: appModules/radiosure/__init__.py:501
msgid "Set volume to middle( 50%)"
msgstr "Poner el volumen al medio (50%)"

#: appModules/radiosure/__init__.py:511
msgid "Say volume"
msgstr "Anuncia el nivel del volumen"

#: appModules/radiosure/__init__.py:518
msgid "Informations copied to clipboard"
msgstr "Las informaciones se copian al Portapapeles."

#: appModules/radiosure/__init__.py:529
msgid ""
"Say the name   of current station and the status line. Twice, copy these "
"informations  to clipboard"
msgstr ""
"Anuncia el nombre de la emisora y la información de la línea de estado. La "
"segunda pulsación copia estas informaciones al Portapapeles."

#: appModules/radiosure/__init__.py:548
msgid "Say buffer"
msgstr "Anuncia la talla del búfer de reproducción."

#: appModules/radiosure/__init__.py:557
msgid "You are in stations list window"
msgstr "Ya estás en la lista de emisoras"

#: appModules/radiosure/__init__.py:566 appModules/radiosure/__init__.py:674
#: appModules/radiosure/__init__.py:753
msgid "Stations list not found"
msgstr "No se pudo encontrar la lista de emisoras"

#: appModules/radiosure/__init__.py:573
msgid ""
"Move focus to stations list. IF stations list is not visible, click Expand "
"button before."
msgstr ""
"Se mueve el foco en la lista de emisoras. Si no está visible, haga clic en "
"el botón Expand para que aparezca."

#: appModules/radiosure/__init__.py:593 appModules/radiosure/__init__.py:617
msgid "Search edit box not found"
msgstr "No se pudo encontrar el cuadro de edición de búsqueda de la emisora."

#: appModules/radiosure/__init__.py:607
msgid "You are in search edit field"
msgstr "Ya estás en el cuadro de edición de búsqueda de la emisora."

#: appModules/radiosure/__init__.py:622
msgid ""
"Move focus to search edit box. IF search edit box is not visible, click "
"Expand button before."
msgstr ""
"Se mueve el foco en el cuadro de edición de búsqueda de la emisora. Si no "
"está visible, haga clic en el botón Expand para que aparezca."

#: appModules/radiosure/__init__.py:678
msgid "Not available because the station list is empty or has only one station"
msgstr "Imposible: la lista de emisoras está vacía o tiene sólo una emisora"

#: appModules/radiosure/__init__.py:689
msgid "Activate context menu to sort stations list by title"
msgstr "Habilitar el menú contextual para ordenar las emisoras por título"

#: appModules/radiosure/__init__.py:698
msgid "Activate context menu to sort stations list by country"
msgstr "Habilitar el menú contextual para ordenar las emisoras por país"

#: appModules/radiosure/__init__.py:707
msgid "Activate context menu to sort stations list by genre"
msgstr "Habilitar el menú contextual para ordenar las emisoras por género"

#: appModules/radiosure/__init__.py:716
msgid "Activate context menu to sort stations list by language"
msgstr "Habilitar el menú contextual para ordenar las emisoras por idioma"

#: appModules/radiosure/__init__.py:783
#, python-format
msgid "None of the %s stations selected randomly could be connected"
msgstr ""
"Ninguna emisora de las %s emisoras seleccionadas al azar  no se pueden "
"conectar."

#: appModules/radiosure/__init__.py:797
msgid "No station in list"
msgstr "No hay ninguna emisora en la lista"

#: appModules/radiosure/__init__.py:803
msgid "Station search running"
msgstr "Búsqueda de una emisora en curso"

#: appModules/radiosure/__init__.py:809
msgid "Start a station randomly"
msgstr ""
"Iniciar la reproducción de una emisora seleccionada al azar en la lista de "
"emisoras"

#: appModules/radiosure/rs_utils.py:27
#, python-format
msgid "%s dialog is allready open"
msgstr "El diálogo %s ya está abierto"

#: appModules/radiosure/rs_utils.py:36
#, python-brace-format
msgid "{addonSummary}'s add-on - {dialogTitle}"
msgstr "{addonSummary} - {dialogTitle}"

#: buildVars.py:24
msgid "radioSure Internet Radio Player: accessibility enhancement"
msgstr ""
"Reproductor de radio por internet RadioSure: complemento de accesibilidad"

#: buildVars.py:28
msgid ""
"This add-on Improves the accessibility of RadioSure, Internet Radio Player "
"with NVDA.\n"
"\tCompatible with RadioSure  2.2.\n"
"\tLower versions are not supported.\n"
"\tSee user manual for more informations.\n"
"\t"
msgstr ""
"Este complemento mejora la accesibilidad del software RadioSure, reproductor "
"de radio por internet, con NVDA.\n"
"\tCompatible con RadioSure 2.2.\n"
"\tLas versiones anteriores no son compatibles.\n"
"\tVéase el manual del usuario para obtener más información.\n"
"\t"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:29
msgid ""
"Some Changes have been made . You must save the configuration and restart "
"NVDA for these changes to take effect. Would you like to do it now?"
msgstr ""
"Se han hecho algunos cambios. Debes guardar la configuración y reiniciar "
"NVDA para que estos cambios tengan efecto. ¿Te gustaría reiniciar ahora?"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:39
msgid "Don't forget to save the configuration for the changes to take effect !"
msgstr ""
"¡No olvide guardar la configuración para que los cambios surtan efecto!"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:46
msgid "Options"
msgstr "Opciones"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:51
msgid "Desactivate progress &bars update"
msgstr "Deshabilitar el seguimiento de las &barras de progreso"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:58
msgid "Random playing"
msgstr "Reproducción aleatoria"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:66
msgid "&Maximum stations to check:"
msgstr "&Número máximo de emisoras a comprobar:"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:75
msgid "&Maximum delay for waiting connexion (in seconds):"
msgstr "&Tiempo de espera máximo de conexión (en segundos):"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:84
msgid "&Skip stations without connexion"
msgstr "&Saltar las emisoras sin conexión"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:90
msgid "&Clear history of stations without connexion"
msgstr "Borrar el historial de emisoras sin &conexión"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:95
msgid "&Use \"shift+control\" instead of \"alt+control\" for input gestures"
msgstr ""
"&Usar \"Mayúscula+Control\" en lugar de \"Alt+Control\" para las órdenes de "
"los gestos de entrada"

#: globalPlugins/radioSureAccessEnhancement/rs_configGui.py:105
msgid ""
"Are you sure you want  to clearthe history of stations without connexion?"
msgstr ""
"¿Estás seguro que deseas borrar el historial de las emisoras sin conexión?"

